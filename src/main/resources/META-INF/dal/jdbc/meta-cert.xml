<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.zeus.dal.core" gen="true" do-class-suffix="Do">
    <entity name="certificate" table="certificate" alias="c" do-class="CertificateDo">
        <var name="domains" value-type="String[]"/>
        <readsets>
            <readset name="ID_VERSION">
                <member name="id"/>
                <member name="version"/>
            </readset>
        </readsets>
        <query-defs>
            <query name="insert" type="INSERT">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)
                    ]]>
                </statement>
            </query>
            <query name="find-by-id" type="SELECT">
                <param name="id"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='id'/> = ${id}
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-domain" type="SELECT" multiple="true">
                <param name="domain"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='domain'/> = ${domain}
                    ]]>
                </statement>
            </query>
            <query name="gross-by-domain-and-state" type="SELECT" multiple="true">
                <param name="domains"/>
                <param name="state"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='domain'/> IN <IN>${domains}</IN>
                        AND <FIELD name='state'/> = ${state}
                    GROUP By domain
                    ]]>
                </statement>
            </query>
            <query name="find-max-by-domain-and-state" type="SELECT">
                <param name="domain"/>
                <param name="state"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='domain'/> = ${domain}
                        AND <FIELD name='state'/> = ${state}
                    ORDER BY id DESC
                    LIMIT 1
                    ]]>
                </statement>
            </query>
            <query name="find-all-max-by-domain-and-state" type="SELECT" multiple="true">
                <param name="domains"/>
                <param name="state"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='domain'/> IN <IN>${domains}</IN>
                        AND <FIELD name='state'/> = ${state}
                    ORDER BY id
                    ]]>
                </statement>
            </query>
            <query name="update-state-by-id" type="UPDATE">
                <param name="id"/>
                <statement>
                    <![CDATA[
                    UPDATE <TABLE/>
                    SET <FIELD name='state'/> = ${state}
                    WHERE <FIELD name='id'/> = ${id}
                    ]]>
                </statement>
            </query>
            <query name="delete-by-id" type="DELETE" batch="true">
                <param name="id"/>
                <statement>
                    <![CDATA[
                    DELETE FROM <TABLE/>
                    WHERE <FIELD name='id'/> = ${id}
                    ]]>
                </statement>
            </query>
        </query-defs>
    </entity>
</entities>

